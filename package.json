{
  "name": "nextauth-client",
  "version": "1.0.0",
  "description": "A CLI tool to scaffold NextAuth.js setup",
  "main": "index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
