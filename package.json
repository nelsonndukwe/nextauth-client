    "@upstash/redis": "latest",
    "commander": "^14.0.0",
    "inquirer": "^12.9.2",
    "next-auth": "^5.0.0",
    "ts-node": "^10.9.2",
    "zod": "^4.0.17",
    "@auth/github": "latest",
    "@auth/google": "latest"
