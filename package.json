{
  "name": "scaffold-auth",
  "version": "1.1.0",
  "description": "A CLI tool to scaffold NextAuth.js setup",
  "main": "index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "tsc -w",
    "link": "npm install -g .",
    "unlink": "npm uninstall -g scaffold-auth",
    "build": "tsc && node scripts/postbuild.js",
    "changeset": "changeset",
    "version-packages": "changeset version",
    "release": "npm run build && npm publish --access public"
  },
  "publishConfig": {
    "access": "public"
  },
  "bin": {
    "scaffold-auth": "./dist/index.js"
  },
  "keywords": [
    "nextauth",
    "authjs",
    "cli",
    "scaffold",
    "auth"
  ],
  "author": "Nelson Ndukwe",
  "license": "MIT",
  "type": "module",
  "devDependencies": {
    "@types/node": "^24.2.1",
    "typescript": "^5.9.2"
  },
  "dependencies": {
    "@auth/upstash-redis-adapter": "latest",
    "@changesets/cli": "^2.29.6",
    "@upstash/redis": "latest",
    "commander": "^14.0.0",
    "inquirer": "^12.9.2",
    "ts-node": "^10.9.2",
    "zod": "^4.0.17"
  }
}
